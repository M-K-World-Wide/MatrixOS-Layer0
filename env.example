# ðŸš€ TrafficFlou Environment Configuration
# Copy this file to .env and configure your settings

# =============================================================================
# CORE SYSTEM CONFIGURATION
# =============================================================================

# Logging and Debug
TRAFFICFLOU_LOG_LEVEL=INFO
TRAFFICFLOU_DEBUG_MODE=false

# Default Target Configuration (GameDin.xyz)
TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz
TRAFFICFLOU_DEFAULT_SESSION_DURATION=600
TRAFFICFLOU_DEFAULT_TRAFFIC_VOLUME=100

# =============================================================================
# AI MODEL CONFIGURATION
# =============================================================================

# OpenAI Configuration
TRAFFICFLOU_OPENAI_API_KEY=your_openai_api_key_here
TRAFFICFLOU_OPENAI_MODEL_NAME=gpt-4
TRAFFICFLOU_OPENAI_MAX_TOKENS=1000
TRAFFICFLOU_OPENAI_TEMPERATURE=0.7
TRAFFICFLOU_OPENAI_TIMEOUT=30
TRAFFICFLOU_OPENAI_MAX_RETRIES=3
TRAFFICFLOU_OPENAI_RETRY_DELAY=1.0
TRAFFICFLOU_OPENAI_CONFIDENCE_THRESHOLD=0.7

# Anthropic Configuration
TRAFFICFLOU_ANTHROPIC_API_KEY=your_anthropic_api_key_here
TRAFFICFLOU_ANTHROPIC_MODEL_NAME=claude-3-sonnet
TRAFFICFLOU_ANTHROPIC_MAX_TOKENS=1000
TRAFFICFLOU_ANTHROPIC_TEMPERATURE=0.7
TRAFFICFLOU_ANTHROPIC_TIMEOUT=30
TRAFFICFLOU_ANTHROPIC_MAX_RETRIES=3
TRAFFICFLOU_ANTHROPIC_RETRY_DELAY=1.0
TRAFFICFLOU_ANTHROPIC_CONFIDENCE_THRESHOLD=0.7

# =============================================================================
# ATHENA AI MODEL CONFIGURATION
# =============================================================================

# Athena AI Model (Required for Phantom Flair)
TRAFFICFLOU_ATHENA_API_KEY=your_athena_api_key_here
TRAFFICFLOU_ATHENA_MODEL_NAME=athena-v2.0
TRAFFICFLOU_ATHENA_ENDPOINT=https://api.athena.ai/v1
TRAFFICFLOU_ATHENA_MODEL_VERSION=athena-v2.0
TRAFFICFLOU_ATHENA_MAX_TOKENS=1000
TRAFFICFLOU_ATHENA_TEMPERATURE=0.7
TRAFFICFLOU_ATHENA_TIMEOUT=30
TRAFFICFLOU_ATHENA_MAX_RETRIES=3
TRAFFICFLOU_ATHENA_RETRY_DELAY=1.0
TRAFFICFLOU_ATHENA_CONFIDENCE_THRESHOLD=0.7

# =============================================================================
# PHANTOM FLAIR CONFIGURATION
# =============================================================================

# Phantom Flair Core Settings
TRAFFICFLOU_PHANTOM_FLAIR_ENABLED=true
TRAFFICFLOU_PHANTOM_FLAIR_INTENSITY=0.8
TRAFFICFLOU_PHANTOM_FLAIR_SOPHISTICATION=4

# Phantom Flair Patterns
TRAFFICFLOU_PHANTOM_FLAIR_PATTERNS=organic_browsing,social_engagement,ecommerce_exploration,content_consumption,research_patterns,casual_navigation,phantom_navigation,flair_interaction,athena_insight

# Phantom Flair Learning and Adaptation
TRAFFICFLOU_PHANTOM_FLAIR_LEARNING=true
TRAFFICFLOU_PHANTOM_FLAIR_ADAPTATION=true

# Phantom Flair Timing and Pacing
TRAFFICFLOU_PHANTOM_FLAIR_MIN_DELAY=0.5
TRAFFICFLOU_PHANTOM_FLAIR_MAX_DELAY=3.0
TRAFFICFLOU_PHANTOM_FLAIR_NATURAL_PACING=true
TRAFFICFLOU_PHANTOM_FLAIR_CONTEXT_AWARE_TIMING=true

# Phantom Flair Interaction Settings
TRAFFICFLOU_PHANTOM_FLAIR_HOVER_EFFECTS=true
TRAFFICFLOU_PHANTOM_FLAIR_SCROLL_BEHAVIOR=true
TRAFFICFLOU_PHANTOM_FLAIR_CLICK_PATTERNS=true
TRAFFICFLOU_PHANTOM_FLAIR_FORM_INTERACTIONS=true

# Phantom Flair Advanced Features
TRAFFICFLOU_PHANTOM_FLAIR_LEARNING_ENABLED=true
TRAFFICFLOU_PHANTOM_FLAIR_PATTERN_ADAPTATION=true
TRAFFICFLOU_PHANTOM_FLAIR_CONTEXT_MEMORY=true
TRAFFICFLOU_PHANTOM_FLAIR_FLAIR_RANDOMIZATION=true
TRAFFICFLOU_PHANTOM_FLAIR_INTENSITY_VARIATION=0.2
TRAFFICFLOU_PHANTOM_FLAIR_PATTERN_EVOLUTION=true

# =============================================================================
# TRAFFIC GENERATION CONFIGURATION
# =============================================================================

# Request Settings
TRAFFICFLOU_TRAFFIC_REQUEST_TIMEOUT=30
TRAFFICFLOU_TRAFFIC_MAX_CONCURRENT_REQUESTS=15
TRAFFICFLOU_TRAFFIC_REQUEST_DELAY=1.0
TRAFFICFLOU_TRAFFIC_MAX_REQUESTS_PER_SESSION=100

# User Agent and Proxy
TRAFFICFLOU_TRAFFIC_USER_AGENT_ROTATION=true
TRAFFICFLOU_TRAFFIC_PROXY_ENABLED=false
TRAFFICFLOU_TRAFFIC_PROXY_LIST=proxy1.example.com:8080,proxy2.example.com:8080

# =============================================================================
# BEHAVIOR SIMULATION CONFIGURATION
# =============================================================================

# Behavior Types and Profiles
TRAFFICFLOU_BEHAVIOR_BEHAVIOR_TYPES=browsing,clicking,scrolling,form_filling
TRAFFICFLOU_BEHAVIOR_USER_PROFILES=casual,focused,researcher,shopper
TRAFFICFLOU_BEHAVIOR_REALISM_LEVEL=4
TRAFFICFLOU_BEHAVIOR_RANDOMIZATION_FACTOR=0.3

# =============================================================================
# ANALYTICS CONFIGURATION
# =============================================================================

# Analytics Core
TRAFFICFLOU_ANALYTICS_ENABLED=true
TRAFFICFLOU_ANALYTICS_STORAGE_BACKEND=memory
TRAFFICFLOU_ANALYTICS_METRICS_INTERVAL=60
TRAFFICFLOU_ANALYTICS_RETENTION_PERIOD=86400
TRAFFICFLOU_ANALYTICS_REAL_TIME_MONITORING=true

# Storage Backends (Optional)
TRAFFICFLOU_ANALYTICS_REDIS_URL=redis://localhost:6379
TRAFFICFLOU_ANALYTICS_INFLUXDB_URL=http://localhost:8086
TRAFFICFLOU_ANALYTICS_ELASTICSEARCH_URL=http://localhost:9200

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# Privacy and Security
TRAFFICFLOU_SECURITY_TRAFFIC_OBFUSCATION=true
TRAFFICFLOU_SECURITY_USER_AGENT_RANDOMIZATION=true
TRAFFICFLOU_SECURITY_PRIVACY_PROTECTION=true
TRAFFICFLOU_SECURITY_REQUEST_ENCRYPTION=false
TRAFFICFLOU_SECURITY_FINGERPRINT_PROTECTION=true

# =============================================================================
# SESSION MANAGEMENT
# =============================================================================

# Session Limits
TRAFFICFLOU_MAX_CONCURRENT_SESSIONS=15
TRAFFICFLOU_SESSION_TIMEOUT=3600
TRAFFICFLOU_DEFAULT_SESSION_DURATION=600

# =============================================================================
# PERFORMANCE CONFIGURATION
# =============================================================================

# System Performance
TRAFFICFLOU_MAX_WORKERS=6
TRAFFICFLOU_REQUEST_RATE_LIMIT=200
TRAFFICFLOU_MEMORY_LIMIT=2048

# =============================================================================
# ADVANCED SETTINGS
# =============================================================================

# Feature Toggles
TRAFFICFLOU_ENABLE_METRICS=true
TRAFFICFLOU_ENABLE_LOGGING=true
TRAFFICFLOU_ENABLE_SECURITY=true

# =============================================================================
# GAMING-SPECIFIC CONFIGURATION
# =============================================================================

# Gaming Mode Settings
TRAFFICFLOU_GAMING_MODE_ENABLED=true
TRAFFICFLOU_GAMING_USER_PROFILES=gamer,casual_player,competitive_player,streamer
TRAFFICFLOU_GAMING_BEHAVIOR_PATTERNS=game_browsing,community_engagement,tournament_research,stream_watching

# =============================================================================
# WEB INTERFACE CONFIGURATION (Optional)
# =============================================================================

# Dashboard Settings
TRAFFICFLOU_DASHBOARD_ENABLED=true
TRAFFICFLOU_DASHBOARD_PORT=8080
TRAFFICFLOU_DASHBOARD_HOST=0.0.0.0

# =============================================================================
# DEVELOPMENT AND TESTING
# =============================================================================

# Development Settings
TRAFFICFLOU_TEST_MODE=false
TRAFFICFLOU_MOCK_AI_RESPONSES=false
TRAFFICFLOU_VERBOSE_LOGGING=false

# =============================================================================
# NOTES AND EXAMPLES
# =============================================================================

# Example configurations for different use cases:

# 1. Basic OpenAI-only setup for GameDin.xyz:
# TRAFFICFLOU_OPENAI_API_KEY=your_key_here
# TRAFFICFLOU_PHANTOM_FLAIR_ENABLED=false
# TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz

# 2. Athena with Phantom Flair for GameDin.xyz (recommended):
# TRAFFICFLOU_ATHENA_API_KEY=your_athena_key_here
# TRAFFICFLOU_PHANTOM_FLAIR_ENABLED=true
# TRAFFICFLOU_PHANTOM_FLAIR_INTENSITY=0.8
# TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz

# 3. Multi-AI model setup for GameDin.xyz:
# TRAFFICFLOU_OPENAI_API_KEY=your_openai_key_here
# TRAFFICFLOU_ANTHROPIC_API_KEY=your_anthropic_key_here
# TRAFFICFLOU_ATHENA_API_KEY=your_athena_key_here
# TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz

# 4. High-performance setup for GameDin.xyz:
# TRAFFICFLOU_MAX_WORKERS=8
# TRAFFICFLOU_MAX_CONCURRENT_SESSIONS=25
# TRAFFICFLOU_REQUEST_RATE_LIMIT=300
# TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz

# 5. Development setup for GameDin.xyz:
# TRAFFICFLOU_DEBUG_MODE=true
# TRAFFICFLOU_LOG_LEVEL=DEBUG
# TRAFFICFLOU_TEST_MODE=true
# TRAFFICFLOU_DEFAULT_TARGET_URL=https://gamedin.xyz 